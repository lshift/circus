#!/usr/bin/env ruby

$LOAD_PATH.unshift File.expand_path('../../bootstrap/bundler/lib', __FILE__)
require "rubygems"
require "bundler"
Bundler.setup

$: << File.expand_path(File.dirname(__FILE__) + "/../lib")
$: << File.expand_path(File.dirname(__FILE__) + "/../vendor/gems/circus/lib")
require File.expand_path(File.dirname(__FILE__) + "/../lib/clown")